{
    "name": "column-jump",
    "displayName": "column Jump",
    "description": "Jump to the next non-empty column",
    "version": "0.0.1",
    "publisher": "CoenraadS",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:column-jump.jumpUp",
        "onCommand:column-jump.jumpDown"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "column-jump.jumpUp",
                "title": "Jump Up to non-empty column"
            },
            {
                "command": "column-jump.jumpDown",
                "title": "Jump Down to non-empty column"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
    ,
    "repository": {
        "type": "git",
        "url": "https://github.com/CoenraadS/column-jump"
    },
    "license": "SEE LICENSE IN LICENSE.md"    
}
